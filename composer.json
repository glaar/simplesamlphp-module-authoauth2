    "keywords" : ["simplesamlphp", "oauth2", "oidc"],
    "license" : "LGPL-2.1-only",
    "require": {
        "php" : ">=5.6",
        "simplesamlphp/composer-module-installer": "^1.1",
        "league/oauth2-client": "^2.3"
